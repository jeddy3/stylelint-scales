{
  "name": "@signal-noise/stylelint-scales",
  "description": "A plugin pack to enforce scales within stylelint",
  "version": "1.5.0",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/signal-noise/stylelint-scales#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/signal-noise/stylelint-scales.git"
  },
  "bugs": {
    "url": "https://github.com/signal-noise/stylelint-scales/issues"
  },
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "spacing",
    "scales",
    "linting",
    "linter",
    "design-system",
    "type-scale"
  ],
  "scripts": {
    "test": "jest",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint \"**/*.js\"",
    "lint:formating": "prettier \"**/*.{js,md}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"**/*.js\" --fix",
    "fix:formating": "prettier \"**/*.{js,md}\" --write",
    "release": "np"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "css-border-property": "^1.1.0",
    "css-global-keywords": "^1.0.1",
    "font-family": "^0.2.0",
    "lodash": "^4.17.20",
    "parse-css-font": "^4.0.0",
    "postcss-values-parser": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1"
  },
  "peerDependencies": {
    "stylelint": "^13.0.0"
  }
}
