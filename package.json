{
  "name": "@signal-noise/stylelint-scales",
  "description": "A plugin pack to enforce scales within Stylelint",
  "version": "2.0.3",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!**/__tests__"
  ],
  "repository": "signal-noise/stylelint-scales",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "spacing",
    "scales",
    "linting",
    "linter",
    "design-system",
    "type-scale"
  ],
  "scripts": {
    "test": "jest",
    "check": "npm-run-all --parallel check:*",
    "check:js": "eslint \"**/*.js\"",
    "check:formating": "prettier \"**/*.{js,md,yml}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"**/*.js\" --fix",
    "fix:formating": "prettier \"**/*.{js,md,yml}\" --write",
    "release": "np"
  },
  "dependencies": {
    "css-global-keywords": "^1.0.1",
    "font-family": "^0.2.0",
    "parse-css-font": "^4.0.0",
    "postcss-values-parser": "^6.0.2"
  },
  "devDependencies": {
    "common-tags": "^1.8.2",
    "eslint": "^8.33.0",
    "jest": "^29.4.2",
    "jest-preset-stylelint": "^6.0.0",
    "np": "^7.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "stylelint": "^15.0.0"
  }
}
