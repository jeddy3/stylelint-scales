{
  "name": "@signal-noise/stylelint-scales",
  "description": "A plugin pack to enforce scales within stylelint",
  "version": "0.1.1",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/signal-noise/stylelint-scales#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/signal-noise/stylelint-scales.git"
  },
  "bugs": {
    "url": "https://github.com/signal-noise/stylelint-scales/issues"
  },
  "keywords": [
    "css",
    "less",
    "sass",
    "scss",
    "stylelint",
    "stylelint-plugin",
    "spacing",
    "scales",
    "font",
    "linting"
  ],
  "scripts": {
    "test": "jest",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint \"**/*.js\"",
    "lint:prettier": "prettier \"**/*.{js,md}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:eslint": "eslint \"**/*.js\" --fix",
    "fix:prettier": "prettier \"**/*.{js,md}\" --write"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "parse-css-font": "^4.0.0",
    "postcss-values-parser": "^3.0.5"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "peerDependencies": {
    "stylelint": "^10.0.0 || ^11.0.0"
  }
}
