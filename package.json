{
  "name": "@signal-noise/stylelint-scales",
  "description": "A plugin pack to enforce scales within stylelint",
  "version": "1.5.0",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!**/__tests__"
  ],
  "repository": "signal-noise/stylelint-scales",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "spacing",
    "scales",
    "linting",
    "linter",
    "design-system",
    "type-scale"
  ],
  "scripts": {
    "test": "jest",
    "check": "npm-run-all --parallel check:*",
    "check:js": "eslint \"**/*.js\"",
    "check:formating": "prettier \"**/*.{js,md,yml}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"**/*.js\" --fix",
    "fix:formating": "prettier \"**/*.{js,md,yml}\" --write",
    "release": "np"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "css-border-property": "^1.1.0",
    "css-global-keywords": "^1.0.1",
    "font-family": "^0.2.0",
    "lodash": "^4.17.20",
    "parse-css-font": "^4.0.0",
    "postcss-values-parser": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "jest-preset-stylelint": "^3.0.0",
    "np": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0"
  },
  "peerDependencies": {
    "stylelint": "^13.0.0"
  }
}
