{
  "name": "stylelint-scales",
  "description": "A Stylelint plugin pack to enforce numeric scales",
  "version": "4.0.0",
  "license": "MIT",
  "exports": "./lib/index.js",
  "files": [
    "lib",
    "!**/__tests__"
  ],
  "type": "module",
  "repository": "jeddy3/stylelint-scales",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "spacing",
    "scales",
    "linting",
    "linter",
    "design-system",
    "type-scale"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "node --test",
    "check": "npm-run-all --parallel check:*",
    "check:js": "eslint \"**/*.js\"",
    "check:formating": "prettier \"**/*.{js,md,yml}\" --check",
    "fix": "npm-run-all --sequential fix:*",
    "fix:js": "eslint \"**/*.js\" --fix",
    "fix:formating": "prettier \"**/*.{js,md,yml}\" --write",
    "release": "np"
  },
  "dependencies": {
    "postcss-values-parser": "^6.0.2"
  },
  "devDependencies": {
    "common-tags": "^1.8.2",
    "eslint": "^8.55.0",
    "np": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "stylelint": "^16.8.2",
    "stylelint-test-rule-node": "^0.2.0"
  }
}
